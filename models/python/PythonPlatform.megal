module python.PythonPlatform

/* Basic Python facts */
PythonPlatform : SoftwareSuite;
    = "https://www.python.org/";
    uses Python;
    uses C.
    
CPython : Software;
    = "https://en.wikipedia.org/wiki/CPython";
    implements Python;
    uses C;
    partOf PythonPlatform.
    
SQLite3 : Library;
    = "https://docs.python.org/2/library/sqlite3.html";
    partOf PythonPlatform;
    uses Python;
    uses SQLite.